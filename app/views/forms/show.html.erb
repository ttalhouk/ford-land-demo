<h2>Forms Page</h2>
<div class="row justify-content-center">
  <div class="col-12 col-sm-6">
    <% if @form.documents.any? %>
      <ul class="list-group">
        <% @form.documents.each do |document| %>
        <%= render 'list', document: document %>
        <% end %>
      </ul>
      <% else %>
      <div class="no-content">
        <p>There are no forms available.  Check back later.</p>
      </div>
    <% end %>
  </div>
</div>

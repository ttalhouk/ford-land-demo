<% @form.documents.each do |document| %>
<tr class='info-row'>
  <th scope="row"><%= document.id %></th>
  <td class="text-left search-field"><%= document.title %></td>
  <td>
    <div class="btn-group" role="group" aria-label="Admin Controls for User">
      <%= link_to "Details", document.doc.url, class:"btn btn-info btn-sm" %>

      <%= link_to "Delete", admin_form_document_path(@form, document), class:"btn btn-danger btn-sm" , method: :delete,  data: { confirm: 'Are you sure?' } %>
    </div>
  </td>
</tr>
<% end %>

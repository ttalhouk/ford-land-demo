<div class="modal-dialog">
  <div class="modal-content">
  <div class="modal-header">
    <h3>Delete Property</h3>
  </div>
  <div class="modal-body">
  <ul class="errors"></ul>
  Are you sure you wish to delete <b><%= @property.name %></b>?
  </div>
  <div class="modal-footer">
    <%= link_to "Yes, Delete This Property", admin_property_path(@property), method: :delete, remote: true, class: "btn btn-danger" %>
    <%= link_to "No, Please Don't", "#", class: "btn btn-outline-secondary", data: { dismiss: "modal" } %>
  </div>
  </div>
</div>
